{
  "name": "dawn-cli-agent",
  "version": "0.1.0",
  "description": "Autonomous task queue processor - dispatches Claude worker agents to generate PRs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dawn": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "prompts/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts",
    "test": "vitest run --exclude tests/pr_creation.test.ts --exclude tests/daytona_sandbox.test.ts --exclude tests/live_preview.test.ts",
    "test:preview": "vitest run tests/live_preview.test.ts",
    "test:integration": "vitest run tests/pr_creation.test.ts tests/daytona_sandbox.test.ts",
    "test:oneshot": "vitest run tests/simple_oneshot_pr_creation.test.ts",
    "test:pr": "vitest run tests/pr_creation.test.ts",
    "test:sandbox": "vitest run tests/daytona_sandbox.test.ts"
  },
  "dependencies": {
    "@daytonaio/sdk": "^0.140.0",
    "dotenv": "^17.2.4",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.18"
  }
}
